{
  "id": "/newscron/newscron-ranker",
  "cmd": null,
  "cpus": 0.2,
  "mem": 1000,
  "disk": 0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "docker-registry.newsriver.io:5000/newscron-ranker:latest",
      "network": "HOST",
      "privileged": false,
      "parameters": [],
      "forcePullImage": true
    }
  },
  "healthChecks": [
    {
      "path": "/health",
      "protocol": "HTTP",
      "portIndex": 0,
      "gracePeriodSeconds": 90,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3,
      "ignoreHttp1xx": false
    }
  ],
  "args": [
    "--threads=20",
    "--queue=40"
  ]
}
